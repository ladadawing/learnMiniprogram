/* 使用主题颜色 */
/* color:var(--themeColor) */
.swiper-container swiper {
  width: 750rpx;
  height: 340rpx;
}
.swiper-container swiper image {
  width: 100%;
}
.cate-container {
  display: flex;
}
.cate-container navigator {
  padding: 20rpx;
  flex: 1;
}
.cate-container navigator image {
  width: 100%;
}
.floor-container .floor-group .floor-title {
  padding: 10rpx 0;
}
.floor-container .floor-group .floor-title image {
  width: 100%;
}
.floor-container .floor-group .floor-list {
  overflow: hidden;
}
.floor-container .floor-group .floor-list navigator {
  float: left;
  width: 33.33%;
}
.floor-container .floor-group .floor-list navigator:nth-last-child(-n+4) {
  height: 27.72711207vw;
  border-left: 10rpx solid #fff;
}
.floor-container .floor-group .floor-list navigator:nth-child(2),
.floor-container .floor-group .floor-list navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.floor-container .floor-group .floor-list navigator image {
  width: 100%;
  height: 100%;
}
